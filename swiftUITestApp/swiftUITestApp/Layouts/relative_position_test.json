{
  "type": "View",
  "width": "matchParent",
  "height": "matchParent",
  "background": "#FFFFFF",
  "orientation": "vertical",
  "child": [
    {
      "data": [
        {
          "name": "title",
          "class": "String",
          "defaultValue": "RelativePositionTest"
        }
      ]
    },
    {
      "type": "Label",
      "id": "title_label",
      "width": "wrapContent",
      "height": "wrapContent",
      "topMargin": 20,
      "text": "@{title}",
      "fontSize": 24,
      "fontColor": "#000000"
    }
  ]
}